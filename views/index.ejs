<div class="album py-5 bg-light">
    <div class="container">
        <div class="row row-cols-1 row-cols-sm-2 row-cols-md-3 g-3" data-masonry='{"percentPosition": true }'>
            <% for(let object of details){ %>
                <div class="col">
                    <div class="card shadow-sm">
                        <img alt="<%= object.project.name %>" src="<%= object.project.image_url %>" class="card-img-top">
                        <div class="card-body">
                            <h5 class="card-title"><%= object.project.name %></h5>
                            <p class="card-text"><%= object.project.summary %></p>
                            <div class="d-flex justify-content-between align-items-center">
                            <div class="btn-group">
                                <button type="button" class="btn btn-sm btn-outline-secondary">View</button>
                            </div>
                            <small class="text-muted"><%= object.owner_name %></small>
                            </div>
                        </div>
                    </div>
                </div>
            <% } %>    
        </div>
    </div>
</div>

